{
    "editor.minimap.enabled": false,
    "editor.renderWhitespace": "selection",
    "editor.fontFamily": "JetBrains Mono",
    "editor.fontSize": 13,
    "editor.fontLigatures": true,
    "files.associations": {
        "Dockerfile_dev": "dockerfile",
        "dotfilesrc": "shellscript",
        "*.inc": "php"
    },
    "terminal.integrated.shell.linux": "/home/linuxbrew/.linuxbrew/bin/zsh",
    "terminal.integrated.shell.osx": "/bin/zsh",
    "terminal.integrated.shell.windows": "C:\\Windows\\System32\\wsl.exe",
    "terminal.integrated.fontFamily": "JetBrains Mono",
    "terminal.integrated.fontSize": 13,
    "terminal.integrated.rendererType": "dom",
    "workbench.colorTheme": "Solarized Dark",
    "workbench.startupEditor": "none",
    "go.formatTool": "goimports",
    "go.testFlags": [
        "-v"
    ],
    "todo-tree.tree.showScanModeButton": false,
    "go.useLanguageServer": true,
    "projectManager.groupList": true,
    "projectManager.sortList": "Name",
    "projectManager.openInNewWindowWhenClickingInStatusBar": true,
    "projectManager.git.baseFolders": [
        "$home\\Code",
    ],
    "editor.codeActionsOnSave": {},
    "window.zoomLevel": 0,
    "dotfiles.repository": "https://github.com/filipenos/dotfiles",
    "dotfiles.installCommand": "~/dotfiles/install-container.sh"
}
